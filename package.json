    "scripts": {
        "build": "yarn install && LANGUAGE=en-US BABEL_ENV=production NODE_ENV=production CI=1 node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js -p --progress --colors --config build/webpack.config.js",
        "build:examples": "LANGUAGE=en-US REACT=true BABEL_ENV=production NODE_ENV=production yarn styleguidist build --config build/styleguide.config.js --mode production",
        "build:i18n": "node ./build/props-to-es2015.js",
        "build:prod": "BABEL_ENV=production NODE_ENV=production yarn webpack --config build/webpack.config.js --mode production",
        "build:selenium": "LANGUAGE=en-US BABEL_ENV=production NODE_ENV=production CI=1 node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js -p --progress --colors --config build/webpack.selenium.config.js",
        "clean": "rm -rf lib functional-tests/lib reports/* i18n/json i18n/*.js styleguide",
        "cy:open": "yarn cy:wait; yarn cypress open",
        "cy:run": "yarn cy:wait; yarn cypress run --spec test/integration/**/*.test.js",
        "cy:wait": "wait-on http-get://localhost:8001",
        "docs:changelog": "yarn conventional-changelog -i CHANGELOG.md --same-file",
        "functional-tests": "node functional-tests/app.js & node ./node_modules/codeceptjs/bin/codecept.js run --steps",
        "functional-tests-ci": "yarn setup && yarn build:selenium && node ./node_modules/codeceptjs/bin/codecept.js run --steps",
        "lint": "npm-run-all lint:*",
        "lint:js": "NODE_ENV=dev yarn eslint src",
        "lint:scss": "yarn stylelint 'src/**/*.scss'",
        "release:github": "yarn conventional-github-releaser",
        "release:major": "./build/release.sh -m && ./build/publish.sh",
        "release:minor": "./build/release.sh -n && ./build/publish.sh",
        "release:patch": "./build/release.sh -p && ./build/publish.sh",
        "setup": "yarn install && yarn clean && yarn build:i18n",
        "start": "yarn install && LANGUAGE=en-US BABEL_ENV=dev NODE_ENV=dev yarn webpack --watch --progress --colors --config build/webpack.config.js --mode development",
        "start:dev": "yarn setup && LANGUAGE=en-US BABEL_ENV=dev NODE_ENV=dev yarn webpack-dev-server --config build/webpack.config.js --mode development",
        "start:examples": "yarn setup && LANGUAGE=en-US REACT=true BABEL_ENV=dev NODE_ENV=dev node --max_old_space_size=8192 node_modules/react-styleguidist/bin/styleguidist.js server --config build/styleguide.config.js --mode development",
        "test": "BABEL_ENV=test NODE_ENV=test yarn jest",
        "test:e2e": "npm-run-all -p -r start:dev cy:run",
        "test:e2e:open": "npm-run-all -p -r start:dev cy:open",
        "test:watch": "yarn test --watch",
        "upgrade:brui": "./build/upgrade_brui.sh",
        "upgrade:flow-typed": "yarn add flow-typed && yarn flow-typed update -i dev"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -e",
            "pre-commit": "lint-staged",
            "pre-push": "./build/prepush.sh"
        }
    },
    "resolutions": {
        "uglify-es": "3.3.4"
    }
